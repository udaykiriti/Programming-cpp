  "Codeforces Template with debug": {
    "prefix": "cftemplate",
    "body": [
       "/******************************",
      " *    author:  uday           *",
      " *    created: ${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}    *",
      " ******************************/",
      "",
      "// g++ -std=c++17 new.cpp -o new",
      "// ./new",
      "//g++ -std=c++17 -Wall -Wextra -O2 s.cpp -o file-name ",
      "//./file-name",
      "#undef _GLIBCXX_DEBUG",
      "#include <bits/stdc++.h>",
      "using namespace std;",
      "#define endl '\\n'",
      "#define ll long long",
      "#define vi vector<int>",
      "",
      "#ifdef LOCAL",
      "#include \"algo/debug.h\"",
      "#else",
      "#define debug(...) 42",
      "#endif",
      "",
      "void solve(){",
      "   //solution here.........",
      "",
      "}",
      "",
      "int main(){",
      "    //freopen(\"in.txt\",\"r\",stdin);",
      "    ios_base::sync_with_stdio(false);",
      "    cin.tie(0); cout.tie(0);",
      "    int t(1), tcase(0); cin>>t;",
      "    while (++tcase, t--) solve();",
      "}"
    ],
    "description": "Codeforces template with author, timestamp, and debug macros"
  },
