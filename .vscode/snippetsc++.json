  "Codeforces Template with debug": {
    "prefix": "cftemplate",
    "body": [
      "/*",
      "g++ -std=c++17 -Wall -Wextra -O2 -DLOCAL ${TM_FILENAME}  -o ${TM_FILENAME_BASE} ",
      "./${TM_FILENAME_BASE} <in.txt> out.txt",
      "*/",
      "#undef _GLIBCXX_DEBUG",
      "#include <bits/stdc++.h>",
      "using namespace std;",
      "",
      "#ifdef LOCAL",
      "#include \"algo/debug.h\"",
      "#else",
      "#define debug(...) 42",
      "#endif",
      "",
      "void solve(){",
      "  //soLuSHoNN hErE.........",
      "",
      "}",
      "",
      "int main(){",
      "     /*",
      "    freopen(\"in.txt\",\"r\",stdin);",
      "    freopen(\"out.txt\",\"w\",stdout);",
      "     */",
      "    ios_base::sync_with_stdio(false);",
      "    cin.tie(0); cout.tie(0);",
      "    int t(1), tcase(0); cin>>t;",
      "    while (++tcase, t--) solve();",
      "    return 0;",
      "}"
    ],
    "description": "Codeforces template with author, timestamp, and debug macros"
  },
